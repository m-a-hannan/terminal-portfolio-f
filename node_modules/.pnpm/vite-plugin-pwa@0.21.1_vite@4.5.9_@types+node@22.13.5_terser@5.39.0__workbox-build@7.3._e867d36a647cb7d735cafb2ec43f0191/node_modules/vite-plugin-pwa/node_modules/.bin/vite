#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/navnee1h/Documents/GitHub/terminal-portfolio/node_modules/.pnpm/vite@4.5.9_@types+node@22.13.5_terser@5.39.0/node_modules/vite/bin/node_modules:/home/navnee1h/Documents/GitHub/terminal-portfolio/node_modules/.pnpm/vite@4.5.9_@types+node@22.13.5_terser@5.39.0/node_modules/vite/node_modules:/home/navnee1h/Documents/GitHub/terminal-portfolio/node_modules/.pnpm/vite@4.5.9_@types+node@22.13.5_terser@5.39.0/node_modules:/home/navnee1h/Documents/GitHub/terminal-portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/navnee1h/Documents/GitHub/terminal-portfolio/node_modules/.pnpm/vite@4.5.9_@types+node@22.13.5_terser@5.39.0/node_modules/vite/bin/node_modules:/home/navnee1h/Documents/GitHub/terminal-portfolio/node_modules/.pnpm/vite@4.5.9_@types+node@22.13.5_terser@5.39.0/node_modules/vite/node_modules:/home/navnee1h/Documents/GitHub/terminal-portfolio/node_modules/.pnpm/vite@4.5.9_@types+node@22.13.5_terser@5.39.0/node_modules:/home/navnee1h/Documents/GitHub/terminal-portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite@4.5.9_@types+node@22.13.5_terser@5.39.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../vite@4.5.9_@types+node@22.13.5_terser@5.39.0/node_modules/vite/bin/vite.js" "$@"
fi
